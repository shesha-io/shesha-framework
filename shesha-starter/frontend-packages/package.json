{
  "name": "@shesha-io/packages",
  "version": "1.0.0",
  "author": "Shesha",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "scripts": {
    "start": "SET NODE_ENV=development BABEL_ENV=development styleguidist server --open",
    "styleguide:build": "SET NODE_ENV=production BABEL_ENV=production styleguidist build",
    "build": "run-script-os",
    "build:win32": "rimraf dist && SET NODE_ENV=production BABEL_ENV=production npm run build:log && rollup -c",
    "build:default": "rm -rf dist && NODE_ENV=production BABEL_ENV=production npm run build:log && rollup -c",
    "build:watch": "npm run build:log && rollup -c -w",
    "build:log": "echo \"Building\"",
    "lint": "npm run check-types && npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint . --ext .js,.jsx,.tsx,.ts",
    "eslint:fix": "eslint . --ext .js,.jsx,.tsx,.ts --fix",
    "stylelint": "stylelint \"**/*.{css,scss,sass}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "check-types": "tsc",
    "prepare": "npm run build",
    "prerelease": "npm run lint:fix",
    "release": "npm publish",
    "predeploy": "npm run styleguide:build",
    "deploy": "gh-pages -d styleguide"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=5",
    "antd": ">=5",
    "lodash": "^4.17.21",
    "next": ">=14",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "@ant-design/icons": ">=5",
    "axios": "^1.7.2",
    "buffer": "^5.7.1",
    "classnames": "^2.3.1",
    "moment": "^2.30.1",
    "nanoid": "^3.3.6",
    "next": ">=14",
    "qs": "^6.12.0",
    "react-use": "^17.3.1",
    "sortablejs": "^1.14.0",
    "styled-components": "^5.2.1",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@ant-design/nextjs-registry": "^1.0.0",
    "@next/bundle-analyzer": "^14.1.0",
    "@next/eslint-plugin-next": "^14.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@shesha-io/reactjs": "0.40.0",
    "@svgr/rollup": "^7.0.0",
    "@testing-library/react": "^11.2.3",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.6",
    "@types/redux-actions": "^2.3.1",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/eslint-plugin-tslint": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "antd": "5.17.4",
    "dotenv": "^8.2.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jsdoc": "^48.0.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^4.3.7",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^29.7.0",
    "lint-staged": "^10.5.3",
    "nock": "^13.0.6",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux-actions": "^2.6.5",
    "rimraf": "^3.0.2",
    "rollup": "^4.18.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-url": "^3.0.1",
    "run-script-os": "^1.1.6",
    "typescript": "^5.1.6"
  }
}
